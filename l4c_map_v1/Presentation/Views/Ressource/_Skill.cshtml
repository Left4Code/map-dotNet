@using Presentation.Models;
@model RessourceSkills
<div class="form-group">
    <div class="col-md-10">
        <table>
            <tr>
                <td>
                    Name
                </td>
                <td>
                    @Html.EditorFor(model => model.skills1.name, new { htmlAttributes = new { @class = "form-control" } })


                </td>
            </tr>
            <tr>
                <td>Experience</td>
                <td>
                    @Html.EditorFor(model => model.skills1.experience, new { htmlAttributes = new { @class = "form-control" } })
            </td>
        </tr>
        <tr>
            <td>Degree</td>
            <td>
                @Html.EditorFor(model => model.skills1.degree, new { htmlAttributes = new { @class = "form-control" } })
        </td>
    </tr>
    <tr>
        <td>Specialty</td>
        <td>
            @Html.EditorFor(model => model.skills1.specialty, new { htmlAttributes = new { @class = "form-control" } })
    </td>
</tr>
</table>
</div>
</div>
